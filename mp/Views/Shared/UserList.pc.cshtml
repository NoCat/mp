@model IEnumerable<mp.BLL.UserInfo>

@foreach (var user in Model)
{
    <div class="waterfall-item fw236" data-id="@user.ID">
        <a class="avt">
            <img src="@user.BigHead" />
        </a>
        <div class="title text-center">
            @user.Name
        </div>
        <div class="attr text-center">
            (@user.ImageCount)张图片  (@user.FollowerCount)人关注
        </div>
    </div>
}

@if (Model.Count() != 0)
{
    <div class="waterfall-item more"></div>
}