@model IEnumerable<mp.Models.WaterfallItem>

@foreach (var item in Model)
{
    var image=item.Item as mp.Models.ImageInfo;
    
    <a href="/image/@image.ID" class="waterfall-item fw78" data-id="@item.ID">
        <img width="@image.ThumbFW78.Width" height="@image.ThumbFW78.Height" src="@image.ThumbFW78.Url" />
        <div class="cover"></div>
    </a>
}
@if (Model.Count() != 0)
{
    <div class="waterfall-item more"></div>
}