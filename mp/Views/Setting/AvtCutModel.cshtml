@model String
<style>
    .origin .avt-img {
        position: relative;
    }
    .preview-md{
        overflow:hidden;
        margin-top:100px;
    }
    .avt-btn{
        margin-top:100px;
        height:100px;
        display:block;
    }
</style>
<div class="row">
    <div class="avt-cut">
        <div class="origin col-md-7 ">
            <div class="avt-img">
                <img src="@Model?r=@(new Random().Next())" />
            </div>
        </div>
        <div class="col-md-2 col-md-offset-1">
            <div class="preview-md">
                <img id="avt-cut-img" src="@Model?r=@(new Random().Next())" />
            </div>
            <div class="avt-btn">
                <button class="btn btn-primary btn-block submit">确定</button>
                <button class="btn btn-default btn-block avt-close">取消</button>
            </div>
        </div>    
    </div>
</div>
