@model mp.DAL.AdminPixivPickUser

<form class="form-horizontal" action="@Request.RawUrl">
    <div class="modal-body">
        <div class="form-group">
            <label class="col-sm-4">p站用户ID：</label>
            <div class="col-sm-8"><input type="text" name="pixivUserId" class="form-control" value="@Model.PixivUserID"/></div>
        </div>
        <div class="form-group">
            <label class="col-sm-4">p站用户名：</label>
            <div class="col-sm-8"><input type="text" name="pixivUserId" class="form-control" value="@Model.PixivUserName"/></div>
        </div>
        <div class="form-group">
            <label class="col-sm-4">图包ID：</label>
            <div class="col-sm-8"><input type="text" name="packageId" class="form-control" value="@Model.PackageID" /></div>
        </div>
        <div class="form-group">
            <label class="col-sm-4">最后采集时间：</label>
            <div class="col-sm-8"><input type="text" name="lastpicktime" class="form-control" value="@Model.LastPickTime.ToShortDateString()" /></div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
        <button class="btn btn-primary">确定</button>
    </div>
</form>

